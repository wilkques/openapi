{
    "name": "wilkques/openapi",
    "description": "Auto generates the OAS3 documentation for a laravel project",
    "license": "MIT",
    "authors": [
        {
            "name": "Wilkques",
            "email": "wilkques@gmail.com"
        }
    ],
    "require-dev": {
        "php": "^7.4|^8.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "laravel/passport": "^9.4",
        "wilkques/php-helper": "^2.0.0"
    },
    "autoload": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "psr-4": {
            "Wilkques\\OpenAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wilkques\\OpenAPI\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-yaml": "Required to use the YAML output option"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wilkques\\OpenAPI\\OpenAPIServiceProvider"
            ]
        }
    },
    "require": {
        "phpdocumentor/reflection-docblock": "^4.3|^5.2",
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^7.0",
        "ext-json": "*"
    }
}